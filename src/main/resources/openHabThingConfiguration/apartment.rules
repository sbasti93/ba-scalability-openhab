rule "CalculateElectricityCosts1"
when
    Item Wohnraum1_Electricity received update
then
    Wohnraum1_Cost.sendCommand(Wohnraum1_Electricity.state as DecimalType * 0.3)
end

rule "Burning-Alarm1"
when
    Item Wohnraum1_CO2 received update
then
    if(Wohnraum1_CO2.state as DecimalType > 9 && Wohnraum1_Temperature.state as DecimalType > 35){
        Wohnraum1_Fire.sendCommand("Feuer")
    }
end

